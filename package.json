{
  "name": "postcss-grouper",
  "version": "0.0.1",
  "description": "PostCSS plugin that groups selectors together.",
  "main": "dist/index.js",
  "scripts": {
    "lint": "tslint -t verbose -c tslint.json $(find ./src -name *.ts)",
    "test": "ava $(find ./test -name *-spec.js)",
    "compile": "rimraf dist/* && tsc && uglifyjs dist/index.js --in-source-map dist/index.js.map --source-map dist/index.js.map -m -b -o dist/index.js",
    "go": "npm run lint && npm run compile && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maraisr/postcss-grouper.git"
  },
  "bugs": {
    "url": "https://github.com/maraisr/postcss-grouper/issues"
  },
  "homepage": "https://github.com/maraisr/postcss-grouper#readme",
  "keywords": [
    "postcss",
    "selectors",
    "group",
    "grouping",
    "css"
  ],
  "author": "Marais Rossouw <me@marair.com>",
  "license": "MIT",
  "dependencies": {
    "postcss": "^5.1.1"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "rimraf": "^2.5.4",
    "tslint": "^3.14.0",
    "typescript": "^1.8.10",
    "uglify-js": "^2.7.0"
  }
}
